{
    "name": "sunnysideup/download-folder",
    "description": "In the CMS you can set for a folder to be downloadable, if you have permission to view the folder and the files in it you can then download the entire folder as a zip.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "Zip",
        "Download",
        "Folder",
        "Permissions",
        "Assets",
        "Files"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^5.0"
    },
    "require-dev": {
        "silverleague/ideannotator": "*"
    },
    "autoload": {
        "psr-4": {
            "Sunnysideup\\DownloadFolder": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "authors": [
        {
            "name": "Nicolaas Thiemen Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
